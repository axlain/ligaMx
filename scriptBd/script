Select * from partidos; 

-- 6. Tabla Estadísticas de Jugadores por Partido
CREATE TABLE estadisticas_jugadores (
    id_partido INT,
    id_jugador INT,
    goles INT DEFAULT 0,
    asistencias INT DEFAULT 0,
    tarjetas_amarillas INT DEFAULT 0,
    tarjetas_rojas INT DEFAULT 0,
    minutos_jugados INT DEFAULT 0,
    PRIMARY KEY (id_partido, id_jugador),
    FOREIGN KEY (id_partido) REFERENCES partidos(id_partido),
    FOREIGN KEY (id_jugador) REFERENCES jugadores(id_jugador)
);

-- 7. Tabla Estadísticas Generales del Partido
CREATE TABLE estadisticas_partido (
    id_partido INT PRIMARY KEY,
    corners_local INT DEFAULT 0,
    corners_visitante INT DEFAULT 0,
    faltas_local INT DEFAULT 0,
    faltas_visitante INT DEFAULT 0,
    tarjetas_amarillas_local INT DEFAULT 0,
    tarjetas_amarillas_visitante INT DEFAULT 0,
    tarjetas_rojas_local INT DEFAULT 0,
    tarjetas_rojas_visitante INT DEFAULT 0,
    FOREIGN KEY (id_partido) REFERENCES partidos(id_partido)
);

-- 8. Tabla Notificaciones (Para gestionar si un usuario ha sido notificado sobre partidos de sus equipos preferidos)
CREATE TABLE notificaciones (
    id_usuario INT,
    id_partido INT,
    notificado BOOLEAN,
    PRIMARY KEY (id_usuario, id_partido),
    FOREIGN KEY (id_usuario) REFERENCES usuarios(id_usuario),
    FOREIGN KEY (id_partido) REFERENCES partidos(id_partido)
);

-- 9. Vista Resumen de Jugador (Para obtener un resumen rápido de cuántos juegos ha jugado un jugador, goles y asistencias)
CREATE VIEW resumen_jugador AS
SELECT 
    j.id_jugador,
    j.nombre,
    e.nombre AS equipo,
    COUNT(sj.id_partido) AS partidos_jugados,
    SUM(sj.goles) AS goles,
    SUM(sj.asistencias) AS asistencias
FROM jugadores j
JOIN estadisticas_jugadores sj ON j.id_jugador = sj.id_jugador
JOIN equipos e ON j.id_equipo = e.id_equipo
GROUP BY j.id_jugador, e.nombre;
