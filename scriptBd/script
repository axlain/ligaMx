('Alex Gonzalez', 'alex.gonzalez@gmail.com', '123', NOW(), '2002-08-22'),
('Carlos Diaz', 'carlos.diaz@gmail.com', '123', NOW(), '1992-11-03');


-- 2. Tabla Equipos
CREATE TABLE equipos (
    id_equipo INT PRIMARY KEY AUTO_INCREMENT,
    nombre VARCHAR(100),
    logo_url VARCHAR(255), 
    liga VARCHAR(100),
    pais VARCHAR(100)
);
